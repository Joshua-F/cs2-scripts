// 1715
[proc,worldmap_findcoordinmap](wma $wma0, coord $coord1)(coord)
if ($wma0 = null | $coord1 = null) {
	return(null);
}
if (worldmap_coordinmap($wma0, $coord1) = true) {
	return($coord1);
}
while (coordy($coord1) > 0) {
	$coord1 = movecoord($coord1, 0, -1, 0);
	if (worldmap_coordinmap($wma0, $coord1) = true) {
		return($coord1);
	}
}
return(null);
